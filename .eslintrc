{
  "extends": "standard",
  "env": {
    "node": true
  },
  "globals": {
  },
  "rules": {
    "semi": 0,
    "no-trailing-spaces": [2, { "skipBlankLines": false }],
    "quotes": [2, "single", "avoid-escape"],
    "no-undef": 2,
    "no-undef-init": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "no-multiple-empty-lines": [2, { "max": 2, "maxBOF": 0, "maxEOF": 1 }]
  }
}